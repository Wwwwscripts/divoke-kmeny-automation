# Discord webhook URL pro CAPTCHA notifikace
DISCORD_WEBHOOK_CAPTCHA=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Discord webhook URL pro upozornění na útoky
DISCORD_WEBHOOK_ATTACK=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Port pro Control Panel (volitelné, výchozí 3000)
PORT=3000

# Úspora dat na proxy - blokuje obrázky, fonty, CSS (volitelné, výchozí true)
# Ušetří 70-90% dat! Nastavte na 'false' pouze pro debugging.
BLOCK_RESOURCES=true
